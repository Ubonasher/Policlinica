@model IEnumerable<Policlinica.Models.Otdelenie>

@{
    ViewBag.Title = "Index";
}


<h2>Выберете отделение</h2>

<table>
    <tr>
        <th align="center">
            @Html.DisplayNameFor(model => model.Название_отделени)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Название_отделени, "Index", "Vrem", new { idO = item.Id }, null)
            </td>
        </tr>
    }
</table>
